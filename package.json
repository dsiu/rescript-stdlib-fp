{
  "name": "@dsiu/rescript-stdlib-fp",
  "version": "0.0.1",
  "author": "Danny Siu <danny.siu@gmail.com>",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript build",
    "watch": "rescript build -w",
    "test": "rescript && pta 'test/**/*.mjs'",
    "clean:build": "yarn clean && yarn build",
    "clean:watch": "yarn clean && yarn watch",
    "clean:test": "yarn clean && yarn build test",
    "test:watch": "onchange --initial '{test,src}/**/*.mjs' -- yarn test",
    "test:watchexec": "watchexec -r -d 0 -w lib/bs -e log -- yarn test"
  },
  "license": "MIT",
  "keywords": [
    "rescript"
  ],
  "type": "module",
  "dependencies": {
    "@rescript/std": "^10.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.20.0",
    "@dusty-phillips/rescript-zora": "^3.0.0",
    "onchange": "^7.1.0",
    "pta": "^1.0.2",
    "rescript": "^10.0.0",
    "tap-difflet": "^0.7.2",
    "zora": "^5.0.3"
  },
  "files": [
    "bsconfig.json",
    "src/*.res",
    "src/*.resi"
  ]
}
